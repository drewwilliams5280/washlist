<!DOCTYPE html>
<html>
  <head>
    <title>WashList</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
      <nav class = "topnav">
        <%= link_to "Home", "/" %>
        <%= link_to "Dashboard", "/dashboard"%>
        <%= link_to "Log Out", "/logout"%>
      </nav>
    <% flash.each do |name, msg| %>
      <div class= "<%=name%>-flash">
        <p><%= msg %></p>
      </div>
    <% end %>
    <%= yield %>
  </body>
</html>
